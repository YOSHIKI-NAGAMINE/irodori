<header>
  <div class="container">
    <div class="title">
      <h1 class="title-text"><a href="/">IRODORI.</a></h1>
    </div>
    <div>
      <% if user_signed_in? %>
        <%= link_to "MyPage", "/users/#{current_user.id}", class: "MyPage" %>
        <%= link_to 'LogOut', destroy_user_session_path, method: :delete, class: "LogOut" %>
      <% else %>
        <%= link_to 'SignUp', new_user_registration_path, class: "SignUp" %>
        <%= link_to 'logIn', new_user_session_path, class: "logIn" %>
      <% end %>
    </div>
  </div>
</header>
